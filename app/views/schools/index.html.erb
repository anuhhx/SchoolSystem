<h1>List of Schools</h1>
<%= image_tag( "divide_plain.png", :class => "content_divide" )%>
<table>
    <tr>
    <th>School</th>
    <th>County</th>
    </tr>
    <% @schools.each do |school| %>
    <tr>
        <td> <%= school.name %></td>
        <td> <%= school.county.try(:name) %></td>
        <td> <%= link_to 'Information', school_path(school) %></td>
        <td> <%= link_to 'Edit', edit_county_school_path(school.county_id, school) %></td>
        <td><%= link_to 'Delete', school_path(school), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <%end%>
</table>
<br>
<%= image_tag( "divide.png", :class => "content_divide" )%>