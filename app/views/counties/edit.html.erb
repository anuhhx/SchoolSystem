<%= form_for :county, url: county_path(@county), method: :patch do |f| %>
 
  <% if @county.errors.any? %>
  
    <div id="error_explanation">
      <h2>
        <%= pluralize(@county.errors.count, "error") %> prohibited
        this county from being saved:
      </h2>
