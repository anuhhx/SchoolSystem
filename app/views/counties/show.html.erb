<h1><%=@county.name%></h1>
<table>
    <tr>
    <th>Schools</th>
    <th></th>
    </tr>
    <% @county.schools.each do |school| %>
    <tr>
        <td> <%= school.name %></td>
        <td> <%= link_to 'Information', county_school_path(@county, school) %></td>
        <td> <%= link_to 'Edit Information', edit_county_school_path(@county, school) %></td>
        <td><%= link_to 'Delete', county_school_path(@county, school), method: :delete, data: { confirm: 'Are you sure?' } %></td>
   
    </tr>
    <%end%>
</table>
<br>
<%= link_to 'New School', new_county_school_path(@county) %><br>